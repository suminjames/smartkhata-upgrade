<p id="notice"><%= notice %></p>

<h1>Listing Share Transactions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @share_transactions.each do |share_transaction| %>
      <tr>
        <td><%= link_to 'Show', share_transaction %></td>
        <td><%= link_to 'Edit', edit_share_transaction_path(share_transaction) %></td>
        <td><%= link_to 'Destroy', share_transaction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Share transaction', new_share_transaction_path %>
