<p id="notice"><%= notice %></p>

<h1>Listing Share Inventories</h1>

<table>
  <thead>
  <tr>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @share_inventories.each do |share_inventory| %>
      <tr>
        <td><%= link_to 'Show', share_inventory %></td>
        <td><%= link_to 'Edit', edit_share_inventory_path(share_inventory) %></td>
        <td><%= link_to 'Destroy', share_inventory, method: :delete, data: {confirm: 'Are you sure?'} %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>

