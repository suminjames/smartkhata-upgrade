= simple_form_for(@master_setup_interest_rate) do |f|
  = f.error_notification

  .form-inputs
    = f.input :start_date
    = f.input :end_date
    = f.input :interest_type, label: 'Interest Type' do
      = f.select :interest_type, [["Payable", "payable"], ["Receivable", "receivable"]]
    = f.input :rate

  .form-actions
    = f.button :submit
