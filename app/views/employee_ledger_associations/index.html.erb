<p id="notice"><%= notice %></p>

<h1>Listing Employee Ledger Associations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employee_ledger_associations.each do |employee_ledger_association| %>
      <tr>
        <td><%= link_to 'Show', employee_ledger_association %></td>
        <td><%= link_to 'Edit', edit_employee_ledger_association_path(employee_ledger_association) %></td>
        <td><%= link_to 'Destroy', employee_ledger_association, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

