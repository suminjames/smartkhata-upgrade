<% provide(:section_title, "Upload Calendar file") %>
<div>
	<%= form_tag import_files_calendars_path, multipart: true do %>
		<div class="form-group">
			<%= file_field_tag :file %>
		</div>
		<div class="form-group">
			<%= submit_tag "Import" ,   { :class => 'btn btn-primary' } %>
		</div>

	<% end %>
</div>
