<% provide(:section_title, "Upload Close out #{@closeout_type == 'debit' ? 'Debit' : 'Credit' } file") %>
<div>
  <%= form_tag import_files_closeouts_path, multipart: true do %>
      <%= hidden_field_tag 'type', @closeout_type %>
      <div class="form-group">
        <%= file_field_tag :file %>
      </div>
      <div class="form-group">
        <%= submit_tag "Import", {:class => 'btn btn-primary'} %>
      </div>

  <% end %>
</div>