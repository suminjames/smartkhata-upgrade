<% if !@file_list.blank? %>
    <div class="text-center">
      <%= paginate @file_list, :theme => 'twitter-bootstrap-3'%>
      <div class="text-center">
        <%= page_entries_info @file_list %>
      </div>
    </div>

    <div class="panel panel-danger">
      <div class="panel-heading">
        List of Sales Payout (CM05) file uploaded
      </div>
      <div class="panel-body">
        <div class="table-responsive">
          <table class="table table-striped">
            <thead>
            <tr>
              <th>Date of Report</th>
              <th>Date of Report(BS)</th>
              <th>Last Updated</th>
            </tr>
            </thead>
            <tbody>
            <% @file_list.each do |file| %>
                <tr>
                  <td><%= file.settlement_date %> </td>
                  <td><%= ad_to_bs(file.settlement_date) %> </td>
                  <td><%= file.updated_at.to_date %></td>
                </tr>
            <% end %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
<% else %>
    No files uploaded yet
<% end %>
